 <div class="login">
 <%= form_tag '/login' do %>
    <div class="forms"
      <%= label_tag 'Username'%>
      <%= text_field_tag :name, params[:name] %>
      <br>
      <%= label_tag 'Password'%>
      <%= password_field_tag :password %></p>
    </div> 
    <br>
     <center>
      <%= submit_tag "Log In", class: "contact-btn btn btn-outline-secondary" %>
      <%= link_to 'Signup', '/signup', class: "contact-btn btn btn-outline-secondary" %>
      </center>
             
<% end %>
</div>
